{
  "name": "PropertyFinder",
  "indexHtmlPath": "index.html",
  "url": null,
  "js": [
    {
      "path": "touch/sencha-touch.js"
    },
    {
      "path": "app.js",
      "bundle": true,
      "update": "delta"
    }
  ],
	 // Conditional theme logic would go here, but WP theme requires work:
    "css": [
        {
            "path": "resources/css/app.css",
            "update": "delta"
//			"platform": ["chrome", "safari", "ios", "android", "firefox", "blackberry"],
//			"theme": "Default"
        },
		/*
		{
            "path": "resources/css/app.wp.css",
            "update": "delta",
			"platform": ["ie10"],
			"theme": "Windows"
		},
		*/
		{
            "path": "resources/css/additional.css",
            "update": "delta"
        }
    ],
  "appCache": {
    "cache": [
      "index.html"
    ],
    "network": [
      "*"
